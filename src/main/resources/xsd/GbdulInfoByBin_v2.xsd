			<xsd:documentation>Тип описывающий адрес местонахождения  от Комитета Статистики</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RKA" type="xsd:string" minOccurs="0">
				<xsd:annotation>
